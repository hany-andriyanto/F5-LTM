when HTTP_REQUEST {

set host [string tolower [HTTP::host]]
set uri_low [string tolower [HTTP::uri]] 

if { $host contains "devtest.domain.com" } {
SSL::disable serverside
}

if { $uri_low contains "/dwdev/dwweb.dll?dwsession" } {

HTTP::uri [string map {/dwdev/dwweb.dll?dwsession /dwdev/tffcweb20070228.105834.dll?TenFoldFCSession} [HTTP::uri]] 

pool POOL-DEV2
}
}

when HTTP_RESPONSE {

if { [HTTP::header values Location] contains "/dwdev/tffcweb20070228.105834.dll?TenFoldFCSession" } {

HTTP::header replace Location [string map { /dwdev/tffcweb20070228.105834.dll?TenFoldFCSession /dwdev/dwayweb.dll?dwsession } [HTTP::header value Location]]
}
}
